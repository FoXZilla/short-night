{
  "name": "short-night",
  "version": "0.0.22",
  "description": "An engine for drawing timeline graph.",
  "main": "index.js",
  "types": "index.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "fix-lint": "tslint --project tsconfig.json --fix",
    "build-doc": "typedoc ./ --exclude **/node_modules/** --out docs/ --readme ./index.docs.md --tsconfig ./tsconfig.json --mode modules --name \"Short Night TypeScript Modules\" && nodetouch docs/.nojekyll",
    "build": "node-sass ./styles.scss -o ./ && tsc",
    "pub": "npm run build && npm publish",
    "test": "npm run lint && npm run build"
  },
  "files": [
    "Event/*",
    "Axis/*",
    "common/*",
    "extensions/*",
    "styles.css",
    "styles.scss",
    "types.d.ts",
    "Timeline.js",
    "Timeline.ts",
    "index.ts",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FoXZilla/short-night.git"
  },
  "keywords": [
    "shortnight",
    "short-night",
    "engine",
    "timeline"
  ],
  "author": "FoXZilla, pea3nut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FoXZilla/short-night/issues"
  },
  "homepage": "https://github.com/pea3nut/short-night#readme",
  "devDependencies": {
    "node-sass": "^4.11.0",
    "touch": "^3.1.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.0.0"
  },
  "dependencies": {
    "@types/moveto": "^1.8.0",
    "moveto": "^1.8.0"
  }
}
