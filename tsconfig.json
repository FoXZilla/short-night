{
  "compilerOptions": {
    "outDir": "./dist/",
    "strict": true,
    "module": "es6",
    "target": "es5",
    "moduleResolution": "node",
    "lib": ["es2015", "es2017","esnext", "dom"],
    "baseUrl":"./",
    "paths": {
      "@/*" :["./src/*"]
    },
    "allowJs": true
  },
  "exclude": [
    "node_modules"
  ]
}
