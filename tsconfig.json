    "outDir": "dist/",
    "declaration": true,
    "downlevelIteration": true,
    "strict": true,
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": ["esnext", "dom"]
  },
  "include": ["src/"],
  "exclude": ["dist/"]
}